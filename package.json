{
	"name": "effect-blackfriday",
	"version": "0.1.1",
	"description": "An Effect.ts project for Black Friday deals",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"prebuild": "bun run clean",
		"build": "tsc",
		"check": "tsc --noEmit",
		"test": "vitest",
		"test:ci": "vitest run --coverage",
		"dev": "tsc --watch"
	},
	"keywords": [
		"effect",
		"typescript",
		"blackfriday"
	],
	"author": "",
	"license": "MIT",
	"packageManager": "bun@1.0.0",
	"dependencies": {
		"effect": "^3.19.0"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"typescript": "^5.9.0",
		"vitest": "^1.0.0"
	},
	"engines": {
		"node": ">=20"
	}
}